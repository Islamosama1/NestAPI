{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/docs/(.*)", 
      "dest": "/docs" 
    },
    {
      "src": "/(.*)", 
      "dest": "src/main.ts", 
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}

